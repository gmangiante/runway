{
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "runway-api/app.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true
        },
        {
            "name": "Python: GUnicorn",
            "type": "python",
            "cwd": "${workspaceFolder}/runway-api",
            "request": "launch",
            "program": "/home/gabe/anaconda3/bin/gunicorn",
            "gevent": true,
            "args": [
                "app:app",
                "--worker-class",
                "gevent",
                "--bind",
                "0.0.0.0:5000",
                "--reload",
                "--log-level",
                "debug"
            ],
            "console": "integratedTerminal"
        }
    ]
}